{
	"name": "hono-cloudfront",
	"version": "0.2.4",
	"description": "A Hono middleware for extracting CloudFront viewer information when using AWS Lambda@Edge or Lambda Function URLs behind CloudFront.",
	"author": "JA Castro <@ubinatus>",
	"homepage": "https://github.com/ubinatus/hono-cloudfront#readme",
	"bugs": {
		"url": "https://github.com/ubinatus/hono-cloudfront/issues"
	},
	"repository": "github:ubinatus/hono-cloudfront",
	"license": "MIT",
	"keywords": [
		"hono",
		"cloudfront",
		"lambda",
		"middleware",
		"typescript"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@9.15.5",
	"scripts": {
		"build": "tsup src/index.ts --format esm --dts",
		"lint": "biome check",
		"lint:fix": "biome check --fix --unsafe",
		"prepublishOnly": "pnpm run build",
		"test": "vitest run --coverage",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"typecheck": "tsc"
	},
	"peerDependencies": {
		"hono": "^4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@types/node": "^24.1.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"hono": "^4.8.12",
		"tsup": "^8.5.0",
		"tsx": "^4.20.3",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"esbuild"
		]
	}
}
